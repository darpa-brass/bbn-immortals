{
    "applicationIdentifier": "ataklite",
    "instanceIdentifier": "ATAKLite-{sessionIdentifier}_{CCID}-{CID}",
    "deploymentPlatformEnvironment": "android_emulator",
    "buildRoot": "applications/client/ATAKLite/",
    "executableFile": "applications/client/ATAKLite/ATAKLite-debug.apk",
    "applicationDeploymentDirectory": "{deploymentDirectory}{instanceIdentifier}/",
    "packageIdentifier": "com.bbn.ataklite",
    "mainActivity": "com.bbn.ataklite.MainActivity",
    "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
    ],
    "files": {
        "harness/scenarioconductor/configs/templates/baseline/ATAKLite-Config.json": "/sdcard/ataklite/ATAKLite-Config.json",
        "harness/scenarioconductor/configs/samples/sample_image.jpg": "/sdcard/ataklite/sample_image.jpg",
        "harness/scenarioconductor/configs/templates/baseline/env.json": "/sdcard/ataklite/env.json"
    },
    "filesForCleanup": [
        "/sdcard/ataklite"
    ],
    "configurationCustomizations": {
    }
}