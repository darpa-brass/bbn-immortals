{
    // The location where all runtime and result files should be placed relative to the DAS root
    "runtimeRoot" : "RUNTIME_ENVIRONMENT",
    // Where the results of a DAS submission should be placed relative to the DAS root
    "resultRoot" : "RESULT_ENVIRONMENT",

    // Details relating to the codebase to immortalize
    "immortalizationTarget" : {
        // The path of the codebase
        "path" : "../"
    },

    // The environments that are valid for deployment. Be sure to synchronize any changes with platformhelper.py
    "deploymentEnvironments" : {
        "java_local" : {
            "deploymentPlatform" : "java",
            "sdkLevel" : 8
        },
        "android_emulator" : {
            "deploymentPlatform" : "android",
            "sdkLevel" : 23
        },
        "android_docker" : {
            "deploymentPlatform" : "android",
            "sdkLevel" : 23
        },
        "android_dynamicanalysis" : {
            "deploymentPlatform" : "android",
            "sdkLevel" : 21
        },
        "android_staticanalysis" : {
            "deploymentPlatform" : "android",
            "sdkLevel" : 23
        }
    },

    "deploymentApplications" : [
        // Details for obtaining the server binary
        {
            // The application identifier
            "application" : "marti",
            // The instance identifier for multiple instances of the application
            "instanceIdentifier" : "UNDEFINED",
            // The environment this application can run on
            "deploymentPlatformEnvironment" : "java",
            // The directory the server files are located in relative to the target source tree root
            "buildSubdir" : "server/Marti/",
            // The java jar file relative to the buildSubdir relative to the target source tree root
            "executableFile" : "server/Marti/Marti-immortals.jar"
            // The configuration file relative to the target source tree root
        },
        // Details for obtaining the client binary
        {
            // The application identifier
            "application" : "ataklite",
            // The instance identifier for multiple instances of the application
            "instanceIdentifier" : "UNDEFINED",
            // The environment this application can run on
            "deploymentPlatformEnvironment" : "android",
            // The client build directory relative to the target source tree root
            "buildSubdir" : "client/ATAKLite/",
            // The apk file relative to the buildSubdir relative to the target source tree root
            "executableFile" : "client/ATAKLite/ATAKLite-debug.apk",
            "packageIdentifier" : "com.bbn.ataklite",
            "mainActivity" : "com.bbn.ataklite.MainActivity",
            "permissions" : [
                "android.permission.ACCESS_FINE_LOCATION",
                "android.permission.CAMERA",
                "android.permission.WRITE_EXTERNAL_STORAGE",
                "android.permission.READ_EXTERNAL_STORAGE"
            ]
        }
    ],

    // Configuration details for running scenarios against generated results
    "scenarioRunner" : {
        // The configuration relative to the DAS root
        "configurationFile" : "configs/scenarios.json",
        // The android sdk level that will be used

        // Configuration options specific to the droidscope docker instance
        "docker" : {
            // This differs from the normal one for the following reasons:
            // 1.  Droidscope does not support higher than 21
            // 2.  Lower than 23 has issues with multiple emulators running concurrently.
            //"androidSdkLevel" : 21,
            // Scripts that may be used by a docker instance or its derivatives (relative to the DAS root)
            "scripts" : [
                "das-analysis/docker/scripts/ds_hookup_package.expect",
                "das-analysis/docker/scripts/ds_load_tracing_plugin.expect",
                "das-analysis/docker/scripts/ds_start_emulator.sh",
                "das-analysis/docker/scripts/android_staticanalysis.sh"
            ]
        }
    },

    // Fuseki configuration details
    "fuseki" : {
        // The root directory of a fuseki runtime environment
        "root" : "/opt/apache-jena-fuseki-2.3.1/",
        //"root" : "/usr/local/opt/fuseki/bin/",
        // The port Fuseki should run on
        "port" : "3030"
    },

    // Immortals-Repository-Service configuration Details
    "repositoryService": {
        // The root location relative to the DAS root
        "root" : "../brainstorming/spiral0/immortals-repository-service/",
        // THe executable file location relative to the DAS root
        "executableFile" : "../knowledge-repo/knowledge-repo/repository-service/target/immortals-repository-service-boot.war",
        // The port the repository service should run on
        "port" : "9999"
    },

    // das-service configuration
    "dasService" : {
        "root" : "das-service",
        "executableFile" : "das-service/das.jar",
        // The port the das-service should run on
        "port" : "8080"
    }
}
